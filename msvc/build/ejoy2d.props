<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <EJOY2D>$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), ejoy2d.root))</EJOY2D>
    <EJOY2D_OBJ>$(EJOY2D)\obj\$(Configuration)\$(ProjectName)\</EJOY2D_OBJ>
    <EJOY2D_LIB>$(EJOY2D)\lib\$(Configuration)\</EJOY2D_LIB>
  </PropertyGroup>
  <PropertyGroup>
    <IntDir>$(EJOY2D_OBJ)</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(EJOY2D)\src\glew-1.10.0\include\;$(EJOY2D)\src\lua-5.2.3\src\;$(EJOY2D)\include\;$(EJOY2D)\..\lib\</AdditionalIncludeDirectories>
      <WarningLevel>Level3</WarningLevel>
    </ClCompile>
    <Link />
    <ResourceCompile>
      <IgnoreStandardIncludePath>
      </IgnoreStandardIncludePath>
    </ResourceCompile>
    <ResourceCompile>
      <ShowProgress>
      </ShowProgress>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="EJOY2D">
      <Value>$(EJOY2D)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_OBJ">
      <Value>$(EJOY2D_OBJ)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_LIB">
      <Value>$(EJOY2D_LIB)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>